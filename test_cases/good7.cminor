function_call: function string (a: array [] string) = {
	if(a[1] != a[2])
		return a[0];
	else
		return a[1];
	y: array [] string = {"hi","hey"};
	x: string = function_call(y);
}

main: function integer(argc: integer, argv: array [] string) = {
	x: string = "hi";
	z: string = "hey";
	y: array [] string = {x, z};
	if(function_call(y) == "hi")
		return 1;
	else
		return 0;
}
