/* Display fibonnaci numbers from 0 to 45. */
fib: function integer ( x: integer, y: string ) = {
if( x<1 ) { return 0; } else {
if(x<2) { return 1; } else {
return fib(x-1,"hi") + fib(x-2,"hey"); // recursive step
} }}

fib2: function void (x:integer) = {
	print "hey";
}

main: function integer ( argc: integer, argv: array[] string) = {
	str: string = "hey";
	y: integer = fib(1,str);
	z: integer = fib(2,str);
	fib2(y);
	return z + y;
}
