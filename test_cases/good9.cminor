printarray: function integer ( a: array [] integer, size: integer ) = {
	i: integer;
	for( i=0;i<size;i++) {
		print a[i], "\n";
	}
	size = 1;
	return size;
}

a: char = 'a';
str: string = "string";

main: function integer (argc: integer, argv: array[] string) = {
	strings: array [] string = {"hi"};
	ints: array[] integer = {1,2,3,main(1,strings)};
	if(argc >= 1 && argv[0] != str)
		return 0;
	else
		return printarray(ints,argc);
}
